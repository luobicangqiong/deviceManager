package com.soft.serviceimpl;

public interface WeiXiuGuanLiService {

	/*
	 * 查询维修记录表，列出我的任务
	 * 
	 */
	public abstract void queryAllWeixiu();

	/*
	 * 开始任务，修改维修记录表的，状态，时间等
	 * 
	 */
	public abstract void startWeixiu();

	/*
	 * 填写维修报告，修改维修表，添加信息
	 */
	public abstract void fillWriteReport();

	/*
	 * 添加备件申请，填写申请单，在申请表写入记录
	 */
	public abstract void addBeijianApply();

	/*
	 * 编辑备件申请，修改申请单
	 */
	public abstract void editBeiJianApply();

	/*
	 * 撤销申请，备件申请，修改状态为撤销
	 */
	public abstract void DeleteBeiJianApply();

	/*
	 * 确认，申请成功。一旦确认，此申请完成，
	 */
	public abstract void sureBeijianApply();

	/*
	 * 根据申请单的状态等，列出未完成的申请单
	 */
	public abstract void queryBeijianApply();

}
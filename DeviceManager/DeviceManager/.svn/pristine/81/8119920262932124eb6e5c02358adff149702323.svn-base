					</form>
					
				</div>
			</div>
			
			
			
			<form action="" method="post" id="form1">
				
				
			</form>
			
				<div class="panel panel-default">
				<div class="panel-heading">用户基本信息</div>
				<table class="table">
					
					<tr class="table-tr">
						<th>姓名</th><td>200</td>
						<th>省份证号</th><td>200</td>
						<th>公司</th><td>200</td>
					</tr>
						<tr class="table-tr">
					<th>手机号</th><td>200</td>
						<th>座机号</th><td>200</td>
